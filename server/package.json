{
	"name": "fastack-starter-server",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"==== TODO Moving forward, use Taskfile.yml instead of these scripts ====": "",
		"preinstall": "command -v trove && trove configure-codeartifact || echo 'Trove not found. Please install trove. This can be ignored in a CI context'",
		"check": "run-p -cls --aggregate-output format-check lint ts-check test",
		"format": "prettier \"{src,test,tools}/**/*.{js,jsx,ts,tsx,css,json}\" --write",
		"format-check": "prettier \"{src,test,tools}/**/*.{js,jsx,ts,tsx,css,json}\" --check",
		"lint": "eslint --cache \"{src,test,tools}/**/*.{js,jsx,ts,tsx}\"",
		"ts-check": "tsc",
		"ts-build": "tsc -p tsconfig.build.json",
		"test": "jest",
		"gen-all": "run-p -cls --aggregate-output gen-db-ts gen-bulk-imports gen-sync-shared",
		"gen-bulk-imports": "tsx tools/genBulkImports.ts",
		"gen-db-ts": "tsx tools/genTsKnex.ts",
		"gen-sync-shared": "tsx tools/syncDirs.ts --json sync_dirs_config.json",
		"dev": "tsx watch src/main.ts",
		"docker": "docker compose up -d",
		"docker-down": "docker compose down",
		"copy-src": "copyfiles -u 1 -e \"src/**/*.ts\" -e \"src/**/*.js\" \"src/**/*\" tsout",
		"build": "npm run ts-build && npm run copy-src",
		"start": "node tsout/main.js",
		"start-worker": "node tsout/main.js --worker",
		"reset-db": "npm run rollback && npm run migrate-types && npm run seed",
		"migrate": "knex migrate:latest --env main",
		"rollback": "knex migrate:rollback --env main",
		"seed": "knex seed:run --env main",
		"migrate-types": "npm run migrate && run-p gen-db-ts",
		"install-on-ci": "npm install --ignore-scripts",
		"reset-db-test": "npm run rollback-test && npm run migrate-test && npm run seed-test",
		"migrate-test": "knex migrate:latest --env test",
		"rollback-test": "knex migrate:rollback --env test",
		"seed-test": "knex seed:run --env test"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,css,json}": "prettier --write",
		"*.{js,jsx,ts,tsx}": "eslint --fix"
	},
	"devDependencies": {
		"@types/bluebird": "^3.5.37",
		"@types/diff": "^5.0.2",
		"@types/js-yaml": "^4.0.5",
		"@types/mkdirp": "^1.0.2",
		"@types/node-fetch": "^2.6.2",
		"@types/serialize-javascript": "^5.0.2",
		"@types/sinonjs__fake-timers": "^8.1.2",
		"@types/uuid": "^9.0.7",
		"@types/yargs": "^17.0.24",
		"@typescript-eslint/eslint-plugin": "^6.11.0",
		"@typescript-eslint/parser": "^6.11.0",
		"concurrently": "^8.2.1",
		"copyfiles": "^2.4.1",
		"eslint": "^8.53.0",
		"eslint-plugin-import": "^2.29.0",
		"eslint-plugin-jest": "^27.6.0",
		"execa": "^5.1.1",
		"glob": "^10.3.10",
		"lint-staged": "^14.0.1",
		"mkdirp": "^1.0.4",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.0.3",
		"ts-jest": "^29.1.1",
		"tsx": "^4.15.2",
		"typescript": "^5",
		"typescript-eslint": "^0.0.1-alpha.0",
		"yargs": "^17.6.0"
	},
	"babel": {
		"presets": [
			"@babel/preset-typescript",
			[
				"@babel/preset-env",
				{
					"targets": {
						"node": "18"
					}
				}
			]
		]
	},
	"dependencies": {
		"@aops-trove/fast-cookie": "^2.0.0",
		"@aops-trove/fast-ext-request": "^1.0.0",
		"@aops-trove/fast-redis": "^1.0.6",
		"@aops-trove/fast-server-common": "^1.0.5",
		"@aops-trove/fast-test-db": "^1.0.0",
		"@aops-trove/placc-sdk": "^1.0.0",
		"@babel/preset-env": "^7.23.8",
		"@babel/preset-typescript": "^7.23.3",
		"@fastify/cors": "^9.0.1",
		"@fastify/view": "^7.1.0",
		"@sinclair/typebox": "^0.32.31",
		"bluebird": "^3.7.2",
		"dotenv": "^16.0.3",
		"fastify": "^4.26.2",
		"fastify-plugin": "^4.5.1",
		"ioredis": "^5.4.1",
		"js-yaml": "^4.1.0",
		"knex": "^3.1.0",
		"lodash": "^4.17.21",
		"pg": "^8.8.0",
		"pino-pretty": "^9.1.1"
	}
}
